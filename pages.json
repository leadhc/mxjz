        {
          "path": "searchArticle/searchArticle",
          "style": {
            "navigationBarTitleText": "搜索文章"
          }
        },
        {
          "path": "searchResult/searchResult",
          "style": {
            "navigationBarTitleText": "搜索结果",
            "enablePullDownRefresh": true,
            "onReachBottomDistance": 200
          }
        },
        {
          "path": "publishArticle/publishArticle",
          "style": {
            "navigationBarTitleText": "发布文章"
          }
        },
        {
          "path": "myFollow/myFollow",
          "style": {
            "navigationBarTitleText": "我的关注"
          }
        },
        {
          "path": "myLikeandCollect/myLikeandCollect",
